<div class="row">
    <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
		<form role="form">
			<h3>Please Sign Up</h3>
			<hr>
			<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
				<%= devise_error_messages! %>
			<div class="row">
				<div class="col-xs-6 col-sm-6 col-md-6">
					<div class="form-group">
                        <input type="text" name="first_name" id="first_name" class="form-control input-lg" placeholder="First Name" tabindex="1">
					</div>
				</div>
				<div class="col-xs-6 col-sm-6 col-md-6">
					<div class="form-group">
						<input type="text" name="last_name" id="last_name" class="form-control input-lg" placeholder="Last Name" tabindex="2">
					</div>
				</div>
			</div>
			<div class="form-group">
				<input type="text" name="display_name" id="display_name" class="form-control input-lg" placeholder="Display Name" tabindex="3">
			</div>
			<div class="form-group">
				<!-- <input type="email" name="email" id="email" class="form-control input-lg" placeholder="Email Address" tabindex="4"> -->
				  <!-- <div><%= f.label :email %><br /> -->
  						<%= f.email_field :email, autofocus: true, name: "email", id: "email", :class => "form-control input-lg", :placeholder => "Email Address", :tabindex => "4" %></div>
			</div>
            <!-- <div class="form-group">
				<input type="email" name="email" id="email" class="form-control input-lg" placeholder="Confirm Email Address" tabindex="5">
			</div> -->
			<div class="row">
				<div class="col-xs-6 col-sm-6 col-md-6">
					<div class="form-group">
						<!-- <input type="password" name="password" id="password" class="form-control input-lg" placeholder="Password" tabindex="6"> -->
						  <!-- <div><%= f.label :password %><br /> -->
    							<%= f.password_field :password, autocomplete: "off", name: "password", id: "password", :class => "form-control input-lg", placeholder: "Password", tabindex: "6" %></div>
					</div>
				</div>
				<div class="col-xs-6 col-sm-6 col-md-6">
					<div class="form-group">
						<!-- <input type="password" name="password_confirmation" id="password_confirmation" class="form-control input-lg" placeholder="Confirm Password" tabindex="7"> -->
						  <!-- <div><%= f.label :password_confirmation %><br /> -->
    							<%= f.password_field :password_confirmation, autocomplete: "off", name: "password_confirmation", id: "password_confirmation", :class => "form-control input-lg", placeholder: "Confirm Password", tabindex: "7" %></div>
					</div>
				</div>
			</div>
            <br>
            <hr>
			<div class="row">
				<div class="col-xs-3 col-sm-3 col-md-3">

					
					<!-- <span class="button-checkbox">
						<button type="button" class="btn btn-info" tabindex="8">I Agree</button>
                        <input type="checkbox" name="t_and_c" id="t_and_c" class="hidden" value="1">
					</span> -->
				</div>
				<div class="col-xs-9 col-sm-9 col-md-9">
				You agree to the <a href="#" data-toggle="modal" data-target="#t_and_c_m">Terms and Conditions</a> set out by this site, including our Cookie Use.
				</div>
			</div>
			
			<hr>
			<div class="row">
				<!-- <div class="col-xs-6 col-md-6 col-sm-offset-3"><input type="submit" value="Register" class="btn btn-primary btn-block btn-lg" tabindex="8">
				</div> -->
				<div class="col-xs-6 col-md-6 col-sm-offset-3"><%= f.submit "Sign up", :class => "btn btn-primary btn-block btn-lg", tabindex: "8" %></div>
			</div>
			<% end %>

			<%= render "devise/shared/links" %>
		</form>
	</div>
</div>
<!-- Modal -->
<div class="modal fade" id="t_and_c_m" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
				<h4 class="modal-title" id="myModalLabel">Terms & Conditions</h4>
			</div>
			<div class="modal-body">
				<p>Please read this.</p>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary" data-dismiss="modal">I Agree</button>
			</div>
		</div><!-- /.modal-content -->
	</div><!-- /.modal-dialog -->
</div><!-- /.modal -->
